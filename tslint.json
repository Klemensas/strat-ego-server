{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": ["warn"],

        "quotemark": [true, "single", "avoid-escape"],
        "object-literal-sort-keys": false,
        "ordered-imports": [false],
        "member-access": [
            false 
        ],
        "interface-name": [
            false
        ]
    },
    "rulesDirectory": []
}