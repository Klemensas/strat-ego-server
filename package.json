{
  "name": "ffe-server",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && node ./build/",
    "start:watch": "nodemon -e ts --exec npm run start",
    "build": "tsc -p ./src",
    "test": "tsc -p ./src && mocha ./build/**/*.spec.js ./build/*.spec.js  --reporter spec",
    "test:ci": "tsc -p ./src && mocha ./build/**/*.spec.js ./build/*.spec.js --reporter mocha-junit-reporter",
    "dev": "nodemon -e ts --exec ts-node --project ./src ./src",
    "lint": "tslint ./src/**/*.ts"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.0",
    "cors": "^2.8.3",
    "crypto": "0.0.3",
    "errorhandler": "^1.4.3",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "express-sequelize-session": "^0.4.0",
    "express-server-sent-events": "^1.1.0",
    "express-session": "^1.15.3",
    "jsonwebtoken": "^7.4.1",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "node-schedule": "^1.2.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.4.0",
    "sequelize": "^4.3.1",
    "serve-favicon": "^2.4.3",
    "socket.io": "^2.0.3",
    "socketio-jwt": "^4.5.0",
    "uws": "^8.14.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.22",
    "@types/sequelize": "^4.0.55",
    "@types/socket.io": "^1.4.29",
    "@types/uws": "^0.13.0",
    "express-status-monitor": "^0.1.9",
    "nodemon": "^1.9.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3"
  }
}
